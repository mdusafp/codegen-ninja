{
  "name": "@codegen-ninja/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "be:serve": "nx serve backend",
    "fe:serve": "nx serve frontend",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "start": "nx run-many --target=serve --all=true",
    "generate:clear": "rm -rf ./examples/*/src",
    "generate:all": "npm-run-all generate:openapi-* && npm run prettier:examples",
    "generate:hooks": "ts-node ./hooks-generator/index.ts",
    "generate:openapi-to-express": "docker build ./examples/openapi-to-express -t openapi-to-express && docker run --rm -v $PWD/examples/openapi-to-express:/local openapi-to-express",
    "generate:openapi-to-gql-express": "docker build ./examples/openapi-to-gql-express -t openapi-to-gql-express && docker run --rm -v $PWD/examples/openapi-to-gql-express:/local openapi-to-gql-express",
    "generate:openapi-to-gql-schema": "docker build ./examples/openapi-to-gql-schema -t openapi-to-gql-schema && docker run --rm -v $PWD/examples/openapi-to-gql-schema:/local openapi-to-gql-schema",
    "generate:openapi-to-mysql": "docker build ./examples/openapi-to-mysql -t openapi-to-mysql && docker run --rm -v $PWD/examples/openapi-to-mysql:/local openapi-to-mysql",
    "generate:openapi-to-nestjs": "docker build ./examples/openapi-to-nestjs -t openapi-to-nestjs && docker run --rm -v $PWD/examples/openapi-to-nestjs:/local openapi-to-nestjs",
    "generate:openapi-to-postman": "docker build ./examples/openapi-to-postman -t openapi-to-postman && docker run --rm -v $PWD/examples/openapi-to-postman:/local openapi-to-postman",
    "generate:openapi-to-sdk": "docker build ./examples/openapi-to-sdk -t openapi-to-sdk && docker run --rm -v $PWD/examples/openapi-to-sdk:/local openapi-to-sdk",
    "fix:openapi-gql-express": "find . -type f -name '*.graphql' -exec sed -i -r 's/()//g' {} + && find . -type f -name '*.graphql' -exec sed -i -r 's/!!]/!]!/g' {} +",
    "prettier:examples": "prettier ./examples/* --write"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "@prisma/client": "^5.4.2",
    "@swc/helpers": "~0.5.2",
    "apollo3-cache-persist": "^0.14.1",
    "axios": "^1.0.0",
    "graphql": "^16.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "react-router-dom": "6.11.2",
    "react-text-truncate": "^0.19.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "styled-components": "^5.3.6",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/named-operations-object": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.1.0",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@ngneat/falso": "^7.1.1",
    "@nx/devkit": "^17.0.2",
    "@nx/eslint-plugin": "16.10.0",
    "@nx/jest": "17.0.2",
    "@nx/js": "^17.0.2",
    "@nx/linter": "16.10.0",
    "@nx/nest": "16.10.0",
    "@nx/node": "16.10.0",
    "@nx/playwright": "16.10.0",
    "@nx/plugin": "^17.0.2",
    "@nx/react": "^16.10.0",
    "@nx/webpack": "16.10.0",
    "@nx/workspace": "16.10.0",
    "@playwright/test": "^1.36.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@storybook/react": "^7.5.1",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.9",
    "@types/react-is": "18.2.2",
